{
  "name": "enhanced-autogen-mcp",
  "version": "0.3.0",
  "description": "Enhanced AutoGen MCP Server with streaming support and modern architecture",
  "main": "build/enhanced_index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node build/enhanced_index.js",
    "start:stdio": "node build/enhanced_index.js --transport=stdio",
    "start:http": "node build/enhanced_index.js --transport=http --port=3001",
    "dev": "ts-node-esm src/enhanced_index.ts",
    "dev:stdio": "ts-node-esm src/enhanced_index.ts --transport=stdio",
    "dev:http": "ts-node-esm src/enhanced_index.ts --transport=http --port=3001",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.3",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^7.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "mcp": {
    "manifest": "./mcp-manifest.json",
    "main": "./build/enhanced_index.js",
    "transport": ["stdio", "http"]
  },
  "author": "Enhanced AutoGen MCP Contributors",
  "license": "MIT"
}
